	// for ABHA number
	function validateInputDigit(inputField) {
		// Remove non-digit characters from the input
		var digits = inputField.value.replace(/\D/g, '');

		// Check if there are exactly 14 digits
		if (!digits.length === 14) {
			$('.error-for-digit').removeClass('hide');
		} else {
			$('.error-for-digit').addClass('hide');
		}
	}
