   function openNextStep(step){
	    let stepclass = '.step--save--btn--' + step;
	    let stepclassWithoutDot = 'step--save--btn--' + step;
	    if($("button").hasClass(stepclassWithoutDot)){
			$(stepclass).trigger("click");  
		}else{
			step=step+1;
			stepclass = '.step--save--btn--' + step;
			$(stepclass).trigger("click");  
		}
   }
   
   var validateRadioButtonsOnChange = (formname, elementName) => {
      let currentFormValidator = Liferay.Form.get('<portlet:namespace/>'+formname);
      if(currentFormValidator){
         currentFormValidator.formValidator.validateField("<portlet:namespace/>"+elementName);
      }
   }
   
   var scroolToErrorField = (step = null) => {   
		
		if(step){
		
			let stepclass = '.step--save--btn--' + step;
			if( $(stepclass) && $(stepclass).hasClass("collapsed")){
			   $(stepclass).trigger("click");  
			}
		}
		
		$('html,body').animate({
             scrollTop: $(".has-error").prop("scrollHeight")},
        'slow');
		
			
   }
   
   var showErrorModal = (msg = 'Internal server error, please try again') => {
   
        $('.error-msg').html(msg)
